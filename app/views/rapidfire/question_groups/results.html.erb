<% if @id == "B.Tech" or @id == "M.Tech" %>
<div class="breadcrumb flat no-print">
  <a href="/">Home</a></li>
  <a href="/program">Program</a></li>
  <a href="#" class="active">Results</a>
</div>
<% else %>
  <div class="breadcrumb flat no-print">
  <a href="/">Home</a></li>
  <a href="/course">Course</a>
  <a href="/course/<%= @id %>">Survey</a>
  <a href="#" class="active">Results</a>
 </div>
<% end %>
<h1><%= @question_group.name %> Survey Results</h1>
<ul class="nav nav-tabs no-print">
  <li class="active">
    <%= link_to "JSON Results", "#json-results", "data-toggle" => "tab" %>
  </li>
  <li>
    <%= link_to "HTML Results", "#html-results", "data-toggle" => "tab" %>
  </li>
</ul>
<div class="tab-content">
  <div class="tab-pane active" id="json-results">
    <%= render partial: "json_results", locals: { question_group: @question_group } %>
  </div>
  <div class="tab-pane" id="html-results">
    <%= render partial: "html_results", locals: { results: @question_group_results } %>
    <span class="no-print">
    <button onclick="window.print();"  class="btn btn-primary">Show Pdf</button></span>
  </div>
</div>
